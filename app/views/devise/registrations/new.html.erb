<script>
  getProfile();
</script>
<div id="registeration-form-wrapper" class="devise-wrapper">
  <div id="registration-header" class="devise-header">
    <div class="row">
      <div class="col-md-6">
        <h2>Sign up</h2>
      </div>
      <div class="col-md-6 inactive">
        <%= render "devise/shared/links" %>
      </div>
    </div> <!-- row ends -->
  </div> <!-- registration header ends -->

  <div id="registration-form" class="devise-content">
    <%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
      <%= devise_error_messages! %>

      <div class="row">
        <div class="col-md-6">
          <div><%= f.label :first_name %><br />
            <%= f.text_field :first_name, :class=>"half" %></div>
        </div>
        <div class="col-md-6">
          <div><%= f.label :last_name %><br />
            <%= f.text_field :last_name, :class=>"half"%></div>
        </div>
      </div>
      <div><%= f.label :profile_name %><br />
        <%= f.text_field :profile_name %>
      </div>

      <div class="hide"><%= f.label :ivle_id  %><br />
        <%= f.text_field :ivle_id %>
      </div>

      <div><%= f.label :email %><br />
      <%= f.email_field :email %></div>

      <div><%= f.label :password %><br />
      <%= f.password_field :password %></div>

      <div><%= f.label :password_confirmation %><br />
      <%= f.password_field :password_confirmation %></div>

      <div><%= f.submit "Get Started!", :class=>"btn btn-default" %></div>
      <% end %>
  </div>
</div>
